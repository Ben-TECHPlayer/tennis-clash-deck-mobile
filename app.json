{
  "expo": {
    "name": "tennis-clash-deck",
    "slug": "tennis-clash-deck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tennis-clash.png",
    "scheme": "tennisclashdeck",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        // "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/tennis-clash.png"
        // "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ben_tech.tennisclashdeck"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/tennis-clash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tennis-clash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "133042ec-c4d5-4e09-9857-1795113aa7fe"
      }
    }
  }
}
